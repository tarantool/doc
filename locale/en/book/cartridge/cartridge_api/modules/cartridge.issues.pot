msgstr ""

#: ../../doc/book/cartridge/cartridge_api/modules/cartridge.issues.rst:33
msgid "Switchover:"
msgstr ""

#: ../../doc/book/cartridge/cartridge_api/modules/cartridge.issues.rst:35
msgid "warning: \"Consistency on ... isn't reached yet\";"
msgstr ""

