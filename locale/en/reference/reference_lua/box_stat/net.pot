"- CONNECTIONS:\n"
"    current: 1\n"
"    rps: 0\n"
"    total: 1\n"
"  REQUESTS:\n"
"    current: 0\n"
"    rps: 0\n"
"    total: 8\n"
"  REQUESTS_IN_PROGRESS:\n"
"    current: 0\n"
"    rps: 0\n"
"    total: 7\n"
"  SENT:\n"
"    total: 19579\n"
"    rps: 0\n"
"  REQUESTS_IN_STREAM_QUEUE:\n"
"    current: 0\n"
"    rps: 0\n"
"    total: 0\n"
"  STREAMS:\n"
"    current: 0\n"
"    rps: 0\n"
"    total: 0\n"
"  RECEIVED:\n"
"    total: 197\n"
"    rps\n"
"..."
msgstr ""

#: ../../doc/reference/reference_lua/box_stat/net.rst:77
