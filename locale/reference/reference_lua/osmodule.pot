msgid "Get environment variable."
msgstr ""

msgid "Parameters: (string) variable-name = environment variable name."
msgstr ""

msgid "tarantool> os.getenv('PATH')\n"
"---\n"
"- /usr/local/sbin:/usr/local/bin:/usr/sbin\n"
"..."
msgstr ""

