"#!/usr/bin/env tarantool\n"
" \n"
" local json = require('json')\n"
" local t = {}\n"
" t[1] = 1; t[2] = json.NULL; t[3]= json.NULL;\n"
" t[4] = 4; t[5] = json.NULL; t[6]= json.NULL;\n"
" t[6] = 4; t[7] = json.NULL; t[8]= json.NULL;\n"
" t[9] = json.NULL\n"
" t[10] = 10\n"
