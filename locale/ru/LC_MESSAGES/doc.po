"     time_t tv_sec;\n"
"     time_t tv_usec;\n"
" } timeval;\n"
"     int gettimeofday(struct timeval *t, void *tzp);\n"
" ]]\n"
" \n"
" local timeval_buf = ffi.new(\"timeval\")\n"
" local now = function()\n"
"     ffi.C.gettimeofday(timeval_buf, nil)\n"
