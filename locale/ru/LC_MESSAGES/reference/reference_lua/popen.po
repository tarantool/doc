"- stdout: open\n"
"  command: sh -c 'grep foo'\n"
"  stderr: open\n"
"  status:\n"
"    state: alive\n"
"  stdin: open\n"
"  opts:\n"
"    stdout: pipe\n"
"    stdin: pipe\n"
"    group_signal: true\n"
"    keep_child: false\n"
